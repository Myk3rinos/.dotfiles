[time]
disabled = false
time_format = "%R"
style = "189 italic"
format = " [î™ $time]($style) "

# ------------ custom ------------
[custom.app_prec]
symbol = "ï"
style = "189"
when = "[[ ${PWD} = *'app'* || ${PWD} = *'src'* ]]"
