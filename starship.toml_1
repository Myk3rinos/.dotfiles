[custom.git_branch_m]
command = "git branch --show-current"
when =  "git branch --show-current && [[ $(git branch --show-current) != *'master'* ]]"
symbol = " î‚  "
format = "[$symbol]($style)"
style = "048"

[custom.git_branch]
when =  "[[ $(git branch --show-current) == *'master'* ]]"
symbol = " î‚  "
format = "[$symbol]($style)"
style = "060"

[custom.github]
when = "[[ $(git branch -a) == *'remotes/origin'* ]]"
# when = "[[ $(git branch -a) == *'remotes/origin/${git branch --show-current}'* ]]"
command = "git remote -v | head -c 1"
style = "237"
# format = "[$output î¬º ]($style)"
# format = "[$output ó°Œ— ]($style)"
# format = "[$output ó°½œ ]($style)"
#  format = "[$outputî‚» ]($style)"
#  format = "[$outputî‚» ]($style)"
 # format = "[$outputï– ]($style)"
 format = "[$output ó°‘ ]($style)"


[git_status]
format = '([$all_status$ahead_behind]($style))'
style = "white"
conflicted = " ğŸ³"
up_to_date = " [î©£${count}](104)"
untracked = ' [ï‘— ${count}](035)'
ahead = " â‡¡${count}"
