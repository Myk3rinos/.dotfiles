stashed = ' [ï” ${count}](blue)'
modified = ' [ï‘™ ${count}](178)'
staged = ' [+${count}](041)'
renamed = ' [ó°‘• ${count}](blue)'
deleted = ' [ï€” ${count}](196)'

[jobs]
