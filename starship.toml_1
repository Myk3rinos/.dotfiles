style = "236"
# ------------ fill ------------

[character]
success_symbol = ""
error_symbol = "[ï’¿](red)"
vicmd_symbol = "[â®](green)"

[git_state]
format = '\([$state( $progress_current/$progress_total)]($style)\)'
style = "bright-black"

[git_metrics]
disabled = true

[fossil_branch]
symbol = "î«¾ "

[git_branch]
symbol = " ï˜ "
# format = "[$symbol$branch]($style)"
