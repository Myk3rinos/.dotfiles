[os]
style = "white"
disabled = false
format = " [$symbol]($style)"

[custom.username]
command = "echo ${USER:0:1}"
when = "true"
format = "[$output](238)"
# style = "bright-yellow"

[custom.hostname]
command = "echo ${HOST:0:1}"
when = "true"
format = "[$output](238)"
# style = "bright-yellow"

[username]
show_always = true
style_user = "#3954b8 bold"
style_root = "bg:#9A348E"
format = '[$user]($style)'
disabled = false

[hostname]
ssh_symbol = "î¬ "
ssh_only = false
# format = "[$symbol$hostname](bold yellow)"
format = "[$symbol](bold green)"
disabled = false

[directory]
# style = "purple"
# style = "068" # bleu
style = "141" # dracula
truncation_symbol = "\uf07c  "
home_symbol = "\uf015 "
read_only_style = "197"
read_only = " ï€£ "
truncation_length = 1
truncate_to_repo = false
format = " [$path]($style)[$read_only]($read_only_style) "

[directory.substitutions]
"Documents" = "ó°ˆ™ "
"TÃ©lÃ©chargements" = "ï€™ "
"Musique" = "ï€ "
